# Script utils importer. Do not execute from shell!

set -e

if ! command -v busybox > /dev/null
then
    echo "BusyBox is required to work!"
    exit 1
fi

for function in $(busybox --list)
do
    alias "${function}"="busybox ${function}"
done

for util in ${0%/*}/../utils/*
do
    alias "${util##*/}"="${util}"
done
